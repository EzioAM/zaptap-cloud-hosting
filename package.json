{
  "name": "zaptap",
  "version": "2.3.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "analyze:bundle": "node scripts/analyze-bundle.js",
    "optimize:bundle": "NODE_ENV=production expo export --output-dir dist --platform all",
    "build:ios": "NODE_OPTIONS='--loader tsx' eas build --platform ios --profile development",
    "build:android": "NODE_OPTIONS='--loader tsx' eas build --platform android --profile development",
    "prebuild:ios": "expo prebuild --platform ios --clean",
    "prebuild:android": "expo prebuild --platform android --clean",
    "run:ios": "expo run:ios --device",
    "run:ios:clean": "expo run:ios --device --clear",
    "run:android": "expo run:android --device",
    "run:android:clean": "expo run:android --device --clear",
    "dev:android": "expo start --android",
    "dev:ios": "expo start --ios",
    "dev:both": "expo start",
    "research": "node scripts/research-automation.js",
    "research:performance": "node scripts/research-automation.js 'performance optimization'",
    "research:nfc": "node scripts/research-automation.js 'NFC implementation best practices'",
    "research:ux": "node scripts/research-automation.js 'mobile app user experience'",
    "research:weekly": "node scripts/weekly-research-report.js",
    "analyze": "node scripts/analyze-research.js",
    "implement": "node scripts/auto-implement.js",
    "redesign": "node scripts/ui-redesign-automation.js",
    "redesign:home": "node scripts/ui-redesign-automation.js HomeScreen",
    "redesign:profile": "node scripts/ui-redesign-automation.js ProfileScreen",
    "redesign:settings": "node scripts/ui-redesign-automation.js SettingsScreen",
    "test:db": "node scripts/verify-database.mjs",
    "test:auth": "node scripts/test-auth-flow.mjs",
    "apply:auth-fix": "node scripts/apply-auth-fix.js",
    "fix:auth": "npm run apply:auth-fix && npm run test:auth",
    "quick-fix": "node scripts/quick-fix.js",
    "apply:security-fix": "node scripts/apply-security-fix.js",
    "verify:security": "node scripts/verify-security.js",
    "fix:security": "npm run apply:security-fix && npm run verify:security",
    "test:auth-flow": "node scripts/test-auth-flow.js",
    "apply:search-path-fix": "node scripts/apply-search-path-fix.js",
    "verify:search-paths": "node scripts/verify-search-paths.js",
    "fix:search-paths": "npm run apply:search-path-fix && npm run verify:search-paths",
    "implement:redesign": "node scripts/implement-redesign.js",
    "supabase:setup": "node scripts/setup-supabase-roles.js setup",
    "supabase:verify": "node scripts/setup-supabase-roles.js verify",
    "supabase:grant": "node scripts/setup-supabase-roles.js grant",
    "supabase:list": "node scripts/setup-supabase-roles.js list",
    "supabase:storage": "node scripts/setup-storage-bucket.js",
    "test:cloud-storage": "node scripts/test-cloud-storage.js",
    "build:preview": "eas build --platform all --profile preview",
    "build:preview:ios": "eas build --platform ios --profile preview",
    "build:preview:android": "eas build --platform android --profile preview",
    "update:preview": "eas update --branch preview --message \"Updated with AI features\"",
    "update:development": "eas update --branch development --message \"Development update\"",
    "deploy:preview": "npm run update:preview && echo 'Preview updated! Install with: eas build --platform all --profile preview'",
    "deploy:build": "node scripts/deploy-with-ai-features.js build",
    "deploy:update": "node scripts/deploy-with-ai-features.js update",
    "deploy:test": "node scripts/deploy-with-ai-features.js test",
    "setup:sharing": "node scripts/setup-sharing-database.js",
    "migrate:sharing": "node scripts/apply-sharing-migration.js",
    "fix:comments": "node scripts/fix-comments-system.js",
    "diagnose:linking": "node scripts/diagnose-app-linking.js",
    "fix:linking": "node scripts/fix-app-linking.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=__tests__/(services|components|hooks)",
    "test:integration": "jest --testPathPattern=__tests__/integration",
    "test:snapshot": "jest --testPathPattern=__tests__/snapshots",
    "test:snapshot:update": "jest --testPathPattern=__tests__/snapshots --updateSnapshot",
    "test:performance": "jest --testPathPattern=__tests__/performance",
    "test:accessibility": "jest --testPathPattern=__tests__/accessibility",
    "test:ios": "PLATFORM_OS=ios jest",
    "test:android": "PLATFORM_OS=android jest",
    "test:web": "PLATFORM_OS=web jest",
    "test:verbose": "jest --verbose",
    "test:silent": "jest --silent",
    "test:validate": "node scripts/validate-test-suite.js",
    "test:debug": "jest --no-cache --no-coverage --verbose",
    "test:ci": "jest --coverage --watchAll=false --ci"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^7.4.3",
    "@react-navigation/native": "^7.1.15",
    "@react-navigation/native-stack": "^7.3.23",
    "@react-navigation/stack": "^7.4.3",
    "@reduxjs/toolkit": "^2.8.2",
    "@supabase/supabase-js": "^2.52.1",
    "@types/uuid": "^10.0.0",
    "axios": "^1.11.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "expo": "~53.0.20",
    "expo-av": "^15.1.7",
    "expo-background-fetch": "^13.1.6",
    "expo-blur": "^14.1.5",
    "expo-build-properties": "^0.14.8",
    "expo-camera": "~16.1.11",
    "expo-clipboard": "~7.1.5",
    "expo-constants": "^17.1.7",
    "expo-crypto": "^14.1.5",
    "expo-dev-client": "^5.2.4",
    "expo-device": "~7.1.4",
    "expo-document-picker": "^13.1.6",
    "expo-file-system": "^18.1.11",
    "expo-haptics": "~14.1.4",
    "expo-image-manipulator": "^13.1.7",
    "expo-image-picker": "^16.1.4",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.3",
    "expo-location": "^18.1.6",
    "expo-mail-composer": "^14.1.5",
    "expo-media-library": "~17.1.7",
    "expo-notifications": "^0.31.4",
    "expo-random": "^14.0.1",
    "expo-sharing": "^13.1.5",
    "expo-sms": "~13.1.4",
    "expo-speech": "^13.1.7",
    "expo-status-bar": "~2.2.3",
    "expo-task-manager": "~13.1.6",
    "expo-updates": "~0.28.17",
    "react": "19.0.0",
    "react-hook-form": "^7.61.1",
    "react-native": "0.79.5",
    "react-native-chart-kit": "^6.12.0",
    "react-native-draggable-flatlist": "^4.0.3",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-nfc-manager": "^3.16.2",
    "react-native-paper": "^5.14.5",
    "react-native-qrcode-svg": "^6.3.15",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-uuid": "^2.0.3",
    "react-native-view-shot": "^4.0.3",
    "react-redux": "^9.2.0",
    "redux-persist": "^6.0.0",
    "uuid": "^11.1.0",
    "victory-native": "^41.18.0",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@expo/config-plugins": "^10.1.2",
    "@react-native-community/cli": "^19.1.1",
    "@react-native/metro-config": "^0.80.2",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.5.1",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.0.10",
    "babel-plugin-transform-imports": "^2.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-plugin-transform-remove-debugger": "^6.9.4",
    "babel-preset-expo": "~13.0.0",
    "jest": "^29.7.0",
    "jest-expo": "^53.0.0",
    "metro-cache": "^0.80.0",
    "metro-minify-terser": "^0.80.0",
    "react-test-renderer": "19.0.0",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "<rootDir>/__tests__/utils/setupTests.ts"
    ],
    "testMatch": [
      "**/__tests__/**/*.(ts|tsx|js)",
      "**/*.(test|spec).(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@react-native|expo|@expo|@unimodules|react-navigation|@react-navigation|@supabase|react-native-nfc-manager|react-native-qrcode-svg)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/__tests__/**",
      "!src/test/**",
      "!src/**/index.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    },
    "testEnvironment": "node"
  },
  "private": true
}
