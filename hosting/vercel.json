{
  "headers": [
    {
      "source": "/",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/html; charset=utf-8"
        }
      ]
    },
    {
      "source": "/(.*).html",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/html; charset=utf-8"
        }
      ]
    },
    {
      "source": "/.well-known/apple-app-site-association",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/automation/(.*)",
      "destination": "/index.html?automation=$1"
    },
    {
      "source": "/share/(.*)",
      "destination": "/index.html?share=$1"
    },
    {
      "source": "/shared/(.*)",
      "destination": "/index.html?shared=$1"
    },
    {
      "source": "/app/(.*)",
      "destination": "/index.html?app=$1"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}